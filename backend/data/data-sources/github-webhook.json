{
  "id": "github-webhook",
  "name": "GitHub Webhook",
  "description": "GitHub webhook event payload",
  "category": "CI/CD",
  "template": {
    "github": {
      "action": "opened",
      "number": 42,
      "pull_request": {
        "id": 123456789,
        "number": 42,
        "state": "open",
        "title": "Add new feature",
        "user": {
          "login": "developer",
          "id": 12345,
          "type": "User"
        },
        "body": "This PR adds a new feature...",
        "head": {
          "ref": "feature/new-feature",
          "sha": "abc123def456"
        },
        "base": {
          "ref": "main",
          "sha": "def456abc123"
        },
        "merged": false,
        "mergeable": true,
        "labels": [
          {
            "name": "enhancement"
          }
        ]
      },
      "repository": {
        "id": 987654321,
        "name": "my-repo",
        "full_name": "org/my-repo",
        "private": true,
        "owner": {
          "login": "org",
          "type": "Organization"
        }
      },
      "sender": {
        "login": "developer",
        "id": 12345,
        "type": "User"
      }
    }
  }
}

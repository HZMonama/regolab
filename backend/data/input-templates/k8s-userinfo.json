{
  "id": "k8s-userinfo",
  "name": "Kubernetes UserInfo",
  "description": "Kubernetes authentication user info from TokenReview",
  "category": "Kubernetes",
  "template": {
    "username": "system:serviceaccount:default:my-service",
    "uid": "12345678-1234-1234-1234-123456789abc",
    "groups": [
      "system:serviceaccounts",
      "system:serviceaccounts:default",
      "system:authenticated"
    ],
    "extra": {
      "authentication.kubernetes.io/pod-name": ["my-pod-abc123"],
      "authentication.kubernetes.io/pod-uid": ["pod-uid-xyz"]
    }
  }
}
